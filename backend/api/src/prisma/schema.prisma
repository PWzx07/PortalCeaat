datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


generator client {
provider =  "prisma-client-js"
}

enum Users{
  ALUNO
  PROFESSOR
}
model usuarios{
  id          String      @id @default(uuid())
  nome        String
  email       String      @unique
  senha       String
  tipo        Users
  criado_em  DateTime   @default(now())
}